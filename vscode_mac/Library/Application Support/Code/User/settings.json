{
  "editor.formatOnType": false,
  "editor.formatOnSave": true,
  "editor.formatOnSaveMode": "modifications",
  "editor.fontFamily": "'Consolas', 'Droid Sans Mono', monospace",
  "editor.fontSize": 16,
  "editor.renderWhitespace": "boundary",
  "editor.cursorBlinking": "blink",
  "editor.cursorStyle": "underline",
  "editor.detectIndentation": true,
  "editor.tabSize": 2,
  "editor.smoothScrolling": true,
  "editor.minimap.enabled": false,
  "editor.dragAndDrop": false,
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "files.autoSave": "off",
  "telemetry.telemetryLevel": "off",
  "workbench.editor.showTabs": false,
  "workbench.editor.enablePreview": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.colorTheme": "Visual Studio Light",
  "window.menuBarVisibility": "toggle",
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "emmet.syntaxProfiles": {
    "javascript": "jsx"
  },
  "html.format.preserveNewLines": true,
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": false,
    "suppressCommitNotFoundWarning": true,
    "suppressFileNotUnderSourceControlWarning": false,
    "suppressGitVersionWarning": false,
    "suppressLineUncommittedWarning": false,
    "suppressNoRepositoryWarning": false,
  },
  "gitlens.codeLens.enabled": false,
  "gitlens.keymap": "none",
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "explorer.openEditors.visible": 0,
  "git.autofetch": true,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "gitlens.hovers.currentLine.over": "line",
  "editor.gotoLocation.multipleDeclarations": "goto",
  "editor.gotoLocation.multipleDefinitions": "goto",
  "debug.javascript.unmapMissingSources": true,
  "files.associations": {
    "*.mt": "clojure"
  },
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "editor.guides.indentation": false,
  "rust-analyzer.inlayHints.chainingHints.enable": false,
  "rust-analyzer.inlayHints.typeHints.enable": false,
  "rust-analyzer.inlayHints.renderColons": false,
  "rust-analyzer.inlayHints.closingBraceHints.enable": false,
  "rust-analyzer.inlayHints.parameterHints.enable": false,
  "rust-analyzer.lens.implementations.enable": false,
  "workbench.colorCustomizations": {
    "[Visual Studio Light]": {
      "activityBar.activeBorder": "#0f4a85",
      "activityBar.background": "#ffffff",
      "activityBar.border": "#0f4a85",
      "activityBar.foreground": "#292929",
      "activityBar.inactiveForeground": "#292929",
      "activityBarBadge.background": "#0f4a85",
      "activityBarBadge.foreground": "#ffffff",
      "badge.background": "#0f4a85",
      "badge.foreground": "#ffffff",
      "button.background": "#0f4a85",
      "button.border": "#0f4a85",
      "button.foreground": "#ffffff",
      "button.secondaryBackground": "#ffffff",
      "button.secondaryForeground": "#292929",
      "button.separator": "#ffffff66",
      "checkbox.background": "#ffffff",
      "checkbox.border": "#0f4a85",
      "checkbox.foreground": "#292929",
      "commandCenter.activeForeground": "#292929",
      "commandCenter.border": "#0f4a85",
      "commandCenter.foreground": "#292929",
      "contrastActiveBorder": "#0f4a85",
      "contrastBorder": "#0f4a85",
      "dropdown.background": "#ffffff",
      "dropdown.border": "#0f4a85",
      "dropdown.foreground": "#292929",
      "dropdown.listBackground": "#ffffff",
      "icon.foreground": "#292929",
      "input.background": "#ffffff",
      "input.border": "#0f4a85",
      "input.foreground": "#292929",
      "input.placeholderForeground": "#292929b3",
      "menu.background": "#ffffff",
      "menu.border": "#0f4a85",
      "menu.foreground": "#292929",
      "menu.selectionBackground": "#0f4a851a",
      "menu.selectionBorder": "#0f4a85",
      "menu.separatorBackground": "#0f4a85",
      "menubar.selectionBorder": "#0f4a85",
      "menubar.selectionForeground": "#292929",
      "sideBar.background": "#ffffff",
      "sideBar.foreground": "#292929",
      "scrollbarSlider.activeBackground": "#0f4a85",
      "scrollbarSlider.background": "#0f4a8566",
      "scrollbarSlider.hoverBackground": "#0f4a85cc",
      "notificationCenter.border": "#0f4a85",
      "notificationCenterHeader.background": "#ffffff",
      "notificationLink.foreground": "#0f4a85",
      "notificationToast.border": "#0f4a85",
      "notifications.background": "#ffffff",
      "notifications.border": "#ffffff",
      "notifications.foreground": "#292929",
      "keybindingLabel.background": "#00000000",
      "keybindingLabel.border": "#0f4a85",
      "keybindingLabel.bottomBorder": "#292929",
      "keybindingLabel.foreground": "#292929",
      "statusBar.border": "#0f4a85",
      "statusBar.debuggingBackground": "#b5200d",
      "statusBar.debuggingBorder": "#0f4a85",
      "statusBar.debuggingForeground": "#ffffff",
      "statusBar.focusBorder": "#292929",
      "statusBar.foreground": "#292929",
      "statusBar.noFolderBorder": "#0f4a85",
      "statusBar.noFolderForeground": "#292929",
      "statusBar.background": "#ffffff",
      "statusBar.noFolderBackground": "#ffffff",
      "statusBarItem.activeBackground": "#0000002e",
      "statusBarItem.compactHoverBackground": "#00000033",
      "statusBarItem.errorBackground": "#b5200d",
      "statusBarItem.errorForeground": "#ffffff",
      "statusBarItem.focusBorder": "#0f4a85",
      "statusBarItem.hoverBackground": "#0000001f",
      "statusBarItem.prominentBackground": "#00000080",
      "statusBarItem.prominentForeground": "#292929",
      "statusBarItem.remoteBackground": "#0f4a85",
      "statusBarItem.remoteForeground": "#ffffff",
      "statusBarItem.warningBackground": "#895503",
      "statusBarItem.warningForeground": "#ffffff",
      "titleBar.activeBackground": "#ffffff",
      "titleBar.activeForeground": "#292929",
      "titleBar.border": "#0f4a85",
      "titleBar.inactiveForeground": "#292929",
      "editorWidget.background": "#ffffff",
      "editorWidget.border": "#0f4a85",
      "editorWidget.foreground": "#292929",
    }
  }
}

